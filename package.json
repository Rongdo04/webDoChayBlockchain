{
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:client": "cd client && npm run dev",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev:client\"",
    "install:all": "npm install && cd backend && npm install && cd ../client && npm install"
  },
  "dependencies": {
    "axios": "^1.12.1",
    "concurrently": "^8.2.2"
  }
}
